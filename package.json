{
  "name": "angular15-json-server",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "generate-db": "node generate-db.js > db.json",
    "db": "npm run generate-db && wait-on db.json && json-server --watch db.json",
    "all": "concurrently \"npm run db\" \"ng serve\"",
    "test": "ng test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.2.4",
    "@angular/common": "^15.2.4",
    "@angular/compiler": "^15.2.4",
    "@angular/core": "^15.2.4",
    "@angular/forms": "^15.2.4",
    "@angular/platform-browser": "^15.2.4",
    "@angular/platform-browser-dynamic": "^15.2.4",
    "@angular/router": "^15.2.4",
    "bootstrap": "5.2",
    "bootstrap-icons": "^1.10.3",
    "concurrently": "^7.6.0",
    "jsonwebtoken": "^9.0.0",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.4",
    "@angular/cli": "~15.2.4",
    "@angular/compiler-cli": "^15.2.4",
    "@faker-js/faker": "^7.6.0",
    "@types/jasmine": "~4.0.0",
    "jasmine-core": "~4.3.0",
    "json-server": "^0.17.3",
    "json-server-auth": "^2.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "typescript": "~4.9.5",
    "wait-on": "^7.0.1"
  }
}
